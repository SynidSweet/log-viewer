{
  "name": "log-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run db:init && next build",
    "start": "next start",
    "lint": "next lint",
    "setup:turso": "node scripts/setup-turso.js",
    "verify-env": "node scripts/verify-env.js",
    "db:init": "node scripts/init-db-deploy.js",
    "db:migrate": "node scripts/migrate.js run",
    "db:status": "node scripts/migrate.js status",
    "postinstall": "npm run db:init",
    "test:performance": "npx jest --config=.claude-testing/jest.config.performance.js",
    "benchmark:performance": "node .claude-testing/performance-benchmark.js",
    "ci:performance": "npm run test:performance && npm run benchmark:performance",
    "validate:performance": "node manual-performance-validation.js",
    "monitor:performance": "node .claude-testing/performance-monitor.js",
    "regression:detect": "node .claude-testing/regression-detector.js",
    "performance:full": "npm run monitor:performance",
    "monitor:click-response": "node .claude-testing/click-response-monitor.js",
    "monitor:render-detection": "node .claude-testing/render-detection.js",
    "monitor:dev-performance": "node .claude-testing/dev-performance-monitor.js",
    "performance:dev": "npm run monitor:dev-performance",
    "profile:react": "node .claude-testing/react-devtools-profiler.js monitor",
    "profile:analyze": "node .claude-testing/react-devtools-profiler.js analyze",
    "profile:report": "node .claude-testing/react-devtools-profiler.js report",
    "profile:clear": "node .claude-testing/react-devtools-profiler.js clear",
    "profile:benchmark": "node .claude-testing/react-profiler-benchmark.js benchmark",
    "profile:validate": "node .claude-testing/react-profiler-benchmark.js validate",
    "profile:baseline": "node .claude-testing/react-profiler-benchmark.js baseline",
    "profile:compare": "node .claude-testing/react-profiler-benchmark.js compare",
    "test:react-profiler": "npx jest --config=.claude-testing/jest.config.react-profiler.js",
    "test:performance:integration": "node --expose-gc ./node_modules/.bin/jest --config=.claude-testing/jest.config.integration-performance.js",
    "performance:integration": "npm run test:performance:integration",
    "memory:monitor": "node --expose-gc scripts/memory-monitor.js",
    "test:memory": "node --expose-gc ./node_modules/.bin/jest --config=.claude-testing/jest.config.js --testPathPattern=memory-monitoring.test.js",
    "memory:validate": "npm run test:memory && npm run memory:monitor",
    "memory:full": "npm run memory:validate",
    "monitor:real-time": "node scripts/real-time-performance-monitor.js",
    "monitor:real-time:dashboard": "node scripts/real-time-performance-monitor.js --dashboard",
    "monitor:real-time:alerts": "node scripts/real-time-performance-monitor.js --alerts --threshold 25",
    "monitor:enhanced": "npm run monitor:real-time",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build"
  },
  "dependencies": {
    "@libsql/client": "^0.15.9",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@types/react-window": "^1.8.8",
    "@vercel/kv": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "nanoid": "^5.1.5",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-window": "^1.8.11",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
